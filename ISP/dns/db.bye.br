$TTL 604800
@   IN  SOA ns.bye.br. root.bye.br. (
        1          ; Serial
        604800     ; Refresh
        86400      ; Retry
        2419200    ; Expire
        604800 )   ; Negative Cache TTL

; Servidor de nomes
@       IN  NS  ns.bye.br.

; Registros A
ns      IN  A   192.168.1.8     ; IP do DNS
portal  IN  A   192.168.1.8     ; IP do servidor
mail    IN  CNAME portal
