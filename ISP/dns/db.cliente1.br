$TTL 604800
@   IN  SOA ns.cliente1.br. root.cliente1.br. (
        2          ; Serial
        604801     ; Refresh
        86400      ; Retry
        2419200    ; Expire
        604801 )   ; Negative Cache TTL

; Servidor de nomes
@       IN  NS  ns.cliente1.br.

; Registros A
ns          IN  A   192.168.1.8     ; IP do servidor DNS
portal      IN  A   192.168.1.8     ; IP do servidor principal

; Alias (CNAME)
sign        IN  CNAME portal
hotsite     IN  CNAME portal
